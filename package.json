{
  "name": "@asdf-vm/actions",
  "version": "1.0.0",
  "description": "asdf github actions",
  "repository": "https://github.com/asdf-vm/actions",
  "author": "Victor Borja <vborja@apache.org>",
  "license": "Apache-2.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fmt": "prettier --write \"**/*.{js,json,md,yml}\"",
    "build": "find . -mindepth 2 -maxdepth 2 -name action.yml | xargs -L1 dirname | xargs -L1 basename | xargs -L1 -IF ncc build -m lib/F/main.js -o F"
  },
  "dependencies": {
    "@actions/core": "1.2.1",
    "@actions/exec": "1.0.3",
    "@actions/github": "2.0.1",
    "@actions/io": "1.0.2"
  },
  "devDependencies": {
    "@zeit/ncc": "0.21.0",
    "husky": "4.0.10",
    "prettier": "1.19.1"
  }
}
